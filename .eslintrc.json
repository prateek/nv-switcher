{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"env": { "node": true, "browser": true },
	"globals": {
		"vi": "readonly"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"extends": [
		"eslint:recommended"
	], 
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": "off",
		"@typescript-eslint/no-unused-vars": ["error", { "args": "none" }],
		"no-prototype-builtins": "off"
	}
}